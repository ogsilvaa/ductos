  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IServicio" closeTimeout="00:05:00" openTimeout="00:01:00"
                 receiveTimeout="00:02:00" sendTimeout="00:05:00" maxReceivedMessageSize="9223372036854775807" transferMode="Streamed">
          <readerQuotas maxDepth="9000000" maxStringContentLength="9000000" maxArrayLength="9000000" />
          <security mode="None" />
        </binding>
        <binding maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="32" maxStringContentLength="2147483647" maxArrayLength="2147483647" />
          <security mode="Message" />
        </binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:3990/Servicio.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicio" contract="IServicio" name="BasicHttpBinding_IServicio" />
      <!--<endpoint address="http://joseliza-001-site3.ftempurl.com/Servicio.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicio" contract="IServicio" name="BasicHttpBinding_IServicio" />-->

    </client>
  </system.serviceModel>
  <system.web>
    <customErrors mode="On">
      
    </customErrors>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>
  </system.web>
  <log4net>
    <appender name="MyFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="D:\logs\SisducWeb_%date{yyyyMMdd}.log" type="log4net.Util.PatternString"/>
      <!--<file value="D:\logs\SisducWeb_%date{yyyyMMdd}.log" type="log4net.Util.PatternString"/>-->
      <!--<appendToFile value="true"/>-->
      <param name="AppendToFile" value="true"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level : %logger - %message %newline"/>
      </layout>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="MyFileAppender"/>
    </root>
  </log4net>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
      <remove name="ApplicationInsightsWebTracking" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" preCondition="managedHandler" />
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
