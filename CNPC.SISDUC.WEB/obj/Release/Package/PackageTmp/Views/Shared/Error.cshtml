@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>
<h3>Codigo de Error: @Model.ActionName - @Model.ControllerName - @Model.GetHashCode()</h3>
<h4>Mensaje @Model.Exception.Message - @Model.GetType()</h4>